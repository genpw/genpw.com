{
  "hosting": {
    "public": "dist",
    "headers": [
      {
        "source" : "**",
        "headers" : [
          {
            "key" : "X-XSS-Protection",
            "value" : "1; mode=block"
          },
          {
            "key" : "X-Content-Type-Options",
            "value" : "nosniff"
          },
          {
            "key" : "X-Frame-Options",
            "value" : "DENY"
          }
        ]
      }
    ]
  }
}
