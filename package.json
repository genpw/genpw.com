{
  "name": "genpw.com",
  "description": "Website for genpw",
  "version": "1.0.0",
  "homepage": "https://genpw.com",
  "author": "Matthew Miller <matthew@mi11er.net> (https://mi11er.net)",
  "contributors": [],
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/genpw/genpw.com.git"
  },
  "bugs": {
    "url": "https://github.com/genpw/genpw.com/issues"
  },
  "scripts": {
    "build": "run-s lint clean build:*",
    "build:css": "node-sass src/design/scss/style.scss dist/css/style.css",
    "build:html": "pug -o dist -E html src/design/pug/*.pug",
    "build:js": "webpack --config config/webpack.js",
    "clean": "rimraf dist firebase-debug.log npm-debug.log",
    "deploy": "run-s build deploy:firebase",
    "deploy:firebase": "firebase deploy",
    "lint": "eslint .",
    "lint:fix": "run-s 'lint -- --fix'",
    "start": "run-p watch start:server",
    "start:server": "babel-node lib/bulid.js",
    "watch": "run-p watch:*",
    "watch:css": "run-s 'build:css -- -r -w src/scss'",
    "watch:html": "run-s 'build:html -- -w'",
    "watch:js": "run-s 'build:js -- -w'"
  },
  "dependencies": {
    "babel-cli": "6.23.0",
    "babel-core": "6.23.1",
    "babel-loader": "6.3.2",
    "babel-preset-env": "1.1.10",
    "browser-sync": "2.18.8",
    "commitizen": "2.9.6",
    "cz-conventional-changelog": "2.0.0",
    "eslint": "3.16.1",
    "eslint-config-airbnb-base": "11.1.0",
    "eslint-plugin-import": "2.2.0",
    "firebase-tools": "3.4.0",
    "genpw": "1.0.7",
    "minimist": "1.2.0",
    "modernizr": "3.3.1",
    "modernizr-loader": "1.0.1",
    "node-sass": "4.5.0",
    "npm-run-all": "4.0.2",
    "picnic": "6.3.2",
    "pug": "2.0.0-beta9",
    "pug-cli": "1.0.0-alpha6",
    "rimraf": "2.6.1",
    "umbrellajs": "2.6.7",
    "webpack": "1.13.3"
  },
  "devDependencies": {},
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  }
}
