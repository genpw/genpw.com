{
  "name": "genpw.com",
  "version": "1.0.16",
  "private": true,
  "description": "Website for genpw",
  "homepage": "https://genpw.com",
  "bugs": {
    "url": "https://github.com/genpw/genpw.com/issues"
  },
  "license": "MIT",
  "author": "Matthew Miller <matthew@mi11er.net> (https://mi11er.net)",
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/genpw/genpw.com.git"
  },
  "scripts": {
    "build": "babel-node lib/build.js --buildOnly",
    "clean": "rimraf dist firebase-debug.log npm-debug.log",
    "deploy": "run-s lint clean 'build -- --production' deploy:firebase",
    "postdeploy": "sonarwhal https://genpw.com >sonarwhal.scan",
    "deploy:firebase": "firebase deploy",
    "lint": "run-s lint:*",
    "lint:js": "eslint  --fix --ignore-path .gitignore  .",
    "lint:package.json": "sort-package-json",
    "release": "standard-version -s",
    "start": "babel-node lib/build.js",
    "test": "run-s lint clean build"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true
    },
    "extends": "airbnb-base",
    "plugins": [
      "import"
    ],
    "rules": {
      "complexity": [
        2,
        6
      ],
      "valid-jsdoc": [
        "error"
      ],
      "import/no-extraneous-dependencies": [
        2,
        {
          "devDependencies": true
        }
      ]
    }
  },
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-preset-env": "1.6.1",
    "browser-sync": "2.23.5",
    "commitizen": "2.9.6",
    "cz-conventional-changelog": "2.1.0",
    "eslint": "4.15.0",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-plugin-import": "2.8.0",
    "firebase-tools": "3.16.0",
    "genpw": "1.0.7",
    "jstransformer-pug": "0.3.0",
    "metalsmith": "2.3.0",
    "metalsmith-layouts": "2.0.0",
    "metalsmith-markdown": "0.2.2",
    "metalsmith-sass": "1.5.1",
    "minimist": "1.2.0",
    "modernizr": "3.5.0",
    "modernizr-loader": "1.0.1",
    "node-sass": "4.7.2",
    "npm-run-all": "4.1.2",
    "picnic": "6.4.0",
    "pug": "2.0.0-rc.4",
    "pug-cli": "1.0.0-alpha6",
    "queue": "4.4.2",
    "rimraf": "2.6.2",
    "sonarwhal": "^0.22.1",
    "sort-package-json": "1.7.1",
    "standard-version": "4.3.0",
    "umbrellajs": "2.10.1",
    "webpack": "3.10.0",
    "webpack-merge": "4.1.1"
  },
  "standard-version": {
    "scripts": {
      "posttag": "git push --follow-tags origin master"
    }
  }
}
